{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rank-boosting-website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rank-boosting-website",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/css/bootstrap.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/assets/css/slicknav.min.css",
              "src/assets/css/flaticon.min.css",
              "src/assets/css/progressbar_barfiller.min.css",
              "src/assets/css/gijgo.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/hamburgers.min.css",
              "src/assets/css/animated-headline.min.css",
              "src/assets/css/magnific-popup.min.css",
              "src/assets/css/fontawesome-all.min.css",
              "src/assets/css/themify-icons.min.css",
              "src/assets/css/slick.min.css",
              "src/assets/css/nice-select.min.css",
              "src/assets/css/style.min.css"
            ],
            "scripts": [
              "src/assets/js/vendor/modernizr-3.5.0.min.js",
              "src/assets/js/vendor/jquery-1.12.4.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.slicknav.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/animated.headline.min.js",
              "src/assets/js/jquery.magnific-popup.js",
              "src/assets/js/gijgo.min.js",
              "src/assets/js/jquery.vide.min.js",
              "src/assets/js/jquery.nice-select.min.js",
              "src/assets/js/jquery.sticky.min.js",
              "src/assets/js/jquery.barfiller.min.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/jquery.countdown.min.js",
              "src/assets/js/hover-direction-snake.min.js",
              "src/assets/js/contact.min.js",
              "src/assets/js/jquery.form.min.js",
              "src/assets/js/jquery.validate.min.js",
              "src/assets/js/mail-script.js",
              "src/assets/js/jquery.ajaxchimp.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/main.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "rank-boosting-website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "rank-boosting-website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rank-boosting-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "rank-boosting-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rank-boosting-website:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {}
        }
      }
    }
  },
  "defaultProject": "rank-boosting-website",
  "cli": {
    "analytics": "e8f6c11a-1e1e-471a-8865-f9eb372bd1db"
  }
}