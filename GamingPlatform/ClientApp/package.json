{
  "name": "gamingplatform",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --host 192.168.0.105 --port 44414 --disable-host-check --ssl --ssl-cert %APPDATA%//ASP.NET//https//%npm_package_name%.pem --ssl-key %APPDATA%//ASP.NET//https//%npm_package_name%.key",
    "start:default": "ng serve --host 192.168.0.105 --port 44414 --disable-host-check --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ssr": "ng run GamingPlatform:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.5",
    "@angular/cdk": "^13.3.5",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "~13.3.0",
    "@angular/material": "^13.3.5",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.6",
    "@angular/router": "~13.3.0",
    "@ng-bootstrap/ng-bootstrap": "^12.0.2",
    "@microsoft/signalr": "^6.0.3",
    "@ng-select/ng-select": "^8.1.1",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "jquery": "^3.6.0",
    "ngx-drag-scroll": "^13.0.0-beta.1",
    "ngx-cookie-service": "^13.2.0",
    "popper.js": "^1.16.1",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@types/signalr-no-jquery": "^0.1.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "schematics-scss-migrate": "^1.3.14",
    "typescript": "~4.6.2"
  }
}
